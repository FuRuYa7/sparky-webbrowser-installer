		$DIALOG $TITLE"$LOCAL2" $MSGBOX $TEXT"\n$LOCAL3"
		exit 0
	fi

	# install the web browser
	$DIALOG2 $TEXT"\n$LOCAL7 Icecat\n\n$LOCAL22\n\n$LOCAL9\n$WWW\n\n$LOCAL14" $TITLE"$LOCAL2" $OKEXIT
	if [ $? = 0 ]; then
